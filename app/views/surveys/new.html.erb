<h1>Start a New Survey!</h1>

<%= form_for @survey do |f| %>
<div class="form-group">
  <%= f.label :name, "Survey Name", class: "col-md-1 control-label" %>
   <div class="col-md-2">
  <%= f.text_field :name,  class: "form-control" %>
  </div>
  </div>
  <%= f.submit "Submit", class: "btn btn-warning" %>
<% end %>